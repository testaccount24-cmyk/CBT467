)ATTR
 @ TYPE(OUTPUT) INTENS(LOW)
 # TYPE(OUTPUT) INTENS(HIGH)
)BODY
%------------------------------  FILE LIST -------------------------------------
%Command ===>_ZCMD                                          %Scroll ====> _AMT +
%SEL     Data Set Name                                 Volser  Last Action Type
)MODEL
_Z      @Z                                            @Z     +#Z          @Z
)INIT
 .HELP=TDSINDX1
 .CURSOR=ZCMD
 &AMT='Half'
 &ZCMD=' '
 &TSEL=' '
 &LASTACT=' '
 .ZVARS='(TSEL DSNAME VOLSER LASTA TYPE)'
)PROC
)END
